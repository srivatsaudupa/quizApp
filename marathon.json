{
  "container": {
    "type": "DOCKER",
    "docker": {
      "network": "HOST",
      "image": "vatsaawsdocker/ubuntu"
    },
    "volumes": [
      {
        "containerPath": "/etc/a",
        "hostPath": "/var/data/a",
        "mode": "RO"
      },
      {
        "containerPath": "/etc/b",
        "hostPath": "/var/data/b",
        "mode": "RW"
      }
    ]
  }
}
